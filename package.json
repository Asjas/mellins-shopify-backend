{
  "name": "node-fastify-typescript-template",
  "main": "dist/index.js",
  "author": "A-J Roos <asjas@hey.com>",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "postinstall": "npm run prisma:generate",
    "start": "node dist/index.js",
    "build": "concurrently \"npm run tsc\" \"npm run build:next\"",
    "build:next": "next build",
    "dev": "nodemon",
    "clean": "rimraf dist",
    "test": "tap",
    "test:w": "tap -w",
    "test:inspect": "tap -O -T --node-arg=--inspect-brk",
    "coverage": "npm test -- --coverage-report=lcov",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker-compose down -d",
    "docker:nuke": "docker-compose down -v --rmi all",
    "tsc": "tsc --project tsconfig.server.json",
    "check-types": "tsc",
    "lint": "eslint --ext .ts .",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .eslintignore \"**/*.+(ts)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build"
  },
  "dependencies": {
    "@apollo/client": "3.4.8",
    "@prisma/client": "2.29.1",
    "@shopify/app-bridge-react": "2.0.3",
    "@shopify/polaris": "6.6.0",
    "@tailwindcss/ui": "0.7.2",
    "date-fns": "2.23.0",
    "fastify": "3.20.2",
    "fastify-autoload": "3.8.0",
    "fastify-cookie": "5.3.1",
    "fastify-cors": "6.0.2",
    "fastify-env": "2.1.1",
    "fastify-etag": "2.1.0",
    "fastify-favicon": "3.1.0",
    "fastify-healthcheck": "3.1.0",
    "fastify-helmet": "5.3.2",
    "fastify-nextjs": "6.0.0",
    "fastify-nodemailer": "5.0.0",
    "fastify-plugin": "3.0.0",
    "fastify-session": "5.2.1",
    "fastify-shopify-auth": "0.1.0",
    "fastify-shopify-graphql-proxy": "0.14.0",
    "fastify-static": "^4.2.3",
    "fluent-json-schema": "3.0.1",
    "graphql": "15.5.1",
    "lodash.isequal": "4.5.0",
    "mjml": "4.10.2",
    "mysql": "2.18.1",
    "next": "11.1.3",
    "pdfkit": "0.12.3",
    "pino-pretty": "5.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.12.2",
    "under-pressure": "5.7.0",
    "undici": "5.8.2",
    "use-persisted-state": "0.3.3"
  },
  "devDependencies": {
    "@types/lodash.isequal": "4.5.5",
    "@types/mjml": "4.7.0",
    "@types/mysql": "2.15.19",
    "@types/node": "16.6.1",
    "@types/react": "17.0.18",
    "@types/react-dom": "17.0.9",
    "@types/tap": "15.0.5",
    "autoprefixer": "10.3.1",
    "concurrently": "6.2.1",
    "eslint-config-asjas": "0.0.25",
    "nodemon": "2.0.12",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.6",
    "prisma": "2.29.1",
    "rimraf": "3.0.2",
    "tailwindcss": "2.2.7",
    "tap": "15.0.9",
    "ts-node": "10.2.0",
    "typescript": "4.3.5"
  }
}
